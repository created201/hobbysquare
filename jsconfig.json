{
    "compilerOptions": {
        "baseUrl": ".",
        "paths": {
            "@/(general)/*": ["components/(general)/*"],
            "@/(landing)/*": ["components/(landing)/*"],
            "@/(login)/*": ["__trash__/(login)/*"],
            "@/(account)/*": ["components/(account)/*"],
            "@/(dashboard)/*": ["components/(dashboard)/*"],
            "@/(post)/*": ["components/(post)/*"],
            "@/(contact)/*": ["components/(contact)/*"],
            "@/helpers": ["helpers/index.js"],
            "@/const": ["const/index.js"],
            "@/server/*": ["libs/server/*"],
            "@/client/*": ["libs/client/*"]
        }
    }
}
